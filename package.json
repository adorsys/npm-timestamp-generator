{
  "name": "a-timestamp-generator",
  "version": "0.0.0-sementically-released",
  "description": "A generator for timestamps",
  "main": "src/index.js",
  "scripts": {
    "commit": "git-cz ",
    "coverage": "npm run test -- --cov --coverage-report=text",
    "coverage:html": "npm run test -- --cov --coverage-report=html",
    "coverage:check": "npm run coverage -- --check-coverage --100",
    "test": "tap src/*.test.js -Rspec",
    "test:watch": "onchange 'src/*.js' -i -- npm test",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    },
    "ghooks": {
      "pre-commit": "npm run test && npm run coverage:check"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/adorsys/npm-timestamp-generator.git"
  },
  "keywords": [
    "generator",
    "timestamp"
  ],
  "author": "Gunnar Radzom <gra@adorsys.de> (http://adorsys.de/)",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/adorsys/npm-timestamp-generator/issues"
  },
  "homepage": "https://github.com/adorsys/npm-timestamp-generator#readme",
  "devDependencies": {
    "commitizen": "2.9.6",
    "cz-conventional-changelog": "2.0.0",
    "ghooks": "2.0.0",
    "onchange": "3.2.1",
    "semantic-release": "^6.3.6",
    "tap": "10.3.3",
    "testdouble": "3.0.0"
  }
}
